<div id="tracks">
  <div id="head">
    <div><p>TITLE</p></div>
    <div><p>LISTEN</p></div>
  </div>
  {{#each tracks}}
  <div class="track">
    <div class="name">
      <p><span>{{this.track_number}}. </span>{{this.name}}</p>
    </div>
    <div class="audio">
      <audio controls src="{{this.preview_url}}">
        Your browser does not support the <code>audio</code> element.
      </audio>
    </div>
  </div>
  {{/each}}
</div>